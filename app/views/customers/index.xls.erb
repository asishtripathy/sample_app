<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>

  </tr>
  <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.first_name %></td>
        <td><%= customer.last_name %></td>
        <td><%= customer.addresses.first.try(:address) %></td>
        <td><%= customer.addresses.first.try(:city) %></td>
        <td><%= customer.addresses.first.try(:state) %></td>
      </tr>
  <% end %>
</table>